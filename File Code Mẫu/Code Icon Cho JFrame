void init(){
        String iconPath ="D:\\java\\DuAn1\\src\\main\\java\\icon\\plus.png";
        try {
            //Đọc icon từ đường dẫn
            Image iconImage = ImageIO.read(new File(iconPath));

            //Thiết lập icon cho frame
            this.setIconImage(iconImage);

        } catch (IOException e) {
            e.printStackTrace();
        }
        this.setTitle("Trang Chủ");
        this.setLocationRelativeTo(null);
    }
